{
  "name": "uxi-docs",
  "version": "2.0.0",
  "description": "doc for cluedin-ui",
  "main": "./server/index.js",
  "scripts": {
    "lint:fix": "uxi-cli lint --quiet --fix ",
    "lint": "uxi-cli lint",
    "test": "uxi-cli test",
    "dev": "uxi-cli dev",
    "dev:public": "uxi-cli dev --host 0.0.0.0 --port 8989",
    "build": "uxi-cli build:webpack",
    "deploy-doc": "npm run buildAndBundle && echo '\n\n # now: git push uxilab\n\n' ",
